import com.inc

function myFunc
	print "called in script basic!\n"
	myFunc = 7777
end function

obj = CreateObject("VB6.Sample") 

if obj = 0 then 
    print "CreateObject failed!\n"
else
    retVal = CallByName(obj, "TestCallBack", VbMethod, ADDRESS(myFunc()) )
    print "TestCallBack returned ", retVal, "\n" 
    ReleaseObject(obj)
    print "test complete!\n"
end if 

